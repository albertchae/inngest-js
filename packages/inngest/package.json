      "import": "./esm/deno/*.js"
    },
    "./helpers/*": {
      "types": "./helpers/*.d.ts",
      "require": "./helpers/*.js",
      "import": "./esm/helpers/*.js"
    },
    "./middleware/*": {
      "types": "./middleware/*.d.ts",
      "require": "./middleware/*.js",
      "import": "./esm/middleware/*.js"
    }
  },
  "homepage": "https://github.com/inngest/inngest-js#readme",
