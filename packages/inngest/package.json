      "import": "./esm/hono.js"
    },
    "./api/*": {
      "types": "./api/*.d.ts",
      "require": "./api/*.js",
      "import": "./esm/api/*.js"
    },
    "./components/*": {
      "types": "./components/*.d.ts",
